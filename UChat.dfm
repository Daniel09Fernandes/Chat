  object sql_msg: TADQuery
    Connection = UniMainModule.humanitarian_
    SQL.Strings = (
      
        'Select m.de, u.nome as deNome, m.para,us.nome as paraNome, m.msg' +
        ', m.gid_msg, m.data, m.visualizado,m.hora from tb_chat_msg m'
      '  left join tb_user u on (u.id = m.de )'
      '  left join tb_user us on (m.para = us.id)'
      'where  '
